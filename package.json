{
  "name": "angular-tailwind-storybook",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "tsc --noEmit && eslint . --ext js,ts,json --quiet --fix",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run angular-tailwind-storybook:storybook",
    "build-storybook": "ng run angular-tailwind-storybook:build-storybook",
    "static-storybook": "NODE_ENV=production npm run docs:json && storybook build -s src/assets/favicons/ -c .storybook -o .out"
  },
  "private": false,
  "peerDependencies": {
    "@angular/animations": "^16.1.6",
    "@angular/common": "^16.1.6",
    "@angular/compiler": "^16.1.6",
    "@angular/core": "^16.1.6",
    "@angular/forms": "^16.1.6",
    "@angular/platform-browser": "^16.1.6",
    "@angular/platform-browser-dynamic": "^16.1.6",
    "@angular/router": "^16.1.6",
    "@ngneat/svg-icon": "^3.2.0",
    "@ngneat/tailwind": "^7.0.3",
    "bluebird": "^3.7.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.6",
    "@angular/cli": "^16.1.6",
    "@angular/compiler-cli": "^16.1.6",
    "@angular/elements": "^16.1.6",
    "@babel/core": "^7.14.3",
    "@compodoc/compodoc": "^1.1.11",
    "@ngneat/svg-generator": "^2.0.0",
    "@storybook/addon-actions": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/angular": "^7.5.3",
    "@types/node": "20.10",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@webcomponents/custom-elements": "^1.4.3",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^8.2.2",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-storybook": "^0.6.15",
    "postcss": "^8.4.31",
    "prettier": "^2.3.0",
    "prettier-eslint": "^12.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.3",
    "tailwindcss": "^3.3.5",
    "typescript": "5.0",
    "webpack": "^5.89.0",
    "webpack-dev-server": "^4.15.1",
    "ng-packagr": "^16.0.0"
  }
}
